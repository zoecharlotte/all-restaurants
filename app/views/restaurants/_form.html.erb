<%= form_for(@restaurant) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :rating %>
  <%= f.number_field :rating %>
  <%= f.label :highlights %>
  <%= f.text_field :highlights %>
  <%= f.submit %>
<% end %>
